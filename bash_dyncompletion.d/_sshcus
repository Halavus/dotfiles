_sshcus () 
{ 
    local cur prev opts;
    COMPREPLY=();
    cur="${COMP_WORDS[COMP_CWORD]}";
    prev="${COMP_WORDS[COMP_CWORD-1]}";
    opts="--help --verbose --version";
    if [[ ${cur} == -* ]]; then
        COMPREPLY=($(compgen -W "${opts}" -- ${cur}));
    else
        COMPREPLY=($(compgen -W "$(ls /usr/local/bin/work_in_progress/appdata/)" -- ${cur}));
    fi
}
