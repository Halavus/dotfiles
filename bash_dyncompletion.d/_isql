_isql () 
{ 
    local cur=${COMP_WORDS[COMP_CWORD]};
    local prev=${COMP_WORDS[COMP_CWORD-1]};
    if [[ "$cur" == -* ]]; then
        COMPREPLY=($( compgen -W "- -b -e -F -p -n -v -X -Y -Q -a -A -c -D -E 		  -h -H -i -I -J -K -l -m -M -o -P -R -s -S -t -U -V -w -z -Z" -- $cur ));
    else
        if [ "$prev" == "-S" ]; then
            COMPREPLY=($( compgen -W '$(grep ^[A-Z] $SYBASE/interfaces)' -- $cur ));
        fi;
    fi
}
