_groupmems () 
{ 
    local cur prev;
    COMPREPLY=();
    _get_comp_words_by_ref cur prev;
    case $prev in 
        -a | -d)
            COMPREPLY=($( compgen -u -- "$cur" ));
            return 0
        ;;
        -g)
            COMPREPLY=($( compgen -g -- "$cur" ));
            return 0
        ;;
    esac;
    if [[ "$cur" == -* ]]; then
        COMPREPLY=($( compgen -W '-a -d -p -g -l' -- "$cur" ));
        return 0;
    fi
}
